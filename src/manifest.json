{
	"background": {
		"scripts": [ "background.js" ],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"16": "icon-16.png",
			"32": "icon-32.png",
			"128": "icon-128.png"
		}
	},
	"description": "Select a part of the page and compute the max contrast ratio",
	"icons": {
		"16": "icon-16.png",
		"32": "icon-32.png",
		"128": "icon-128.png"
	},
	"manifest_version": 2,
	"name": "SK Contrast",
	"permissions": [ "activeTab" ],
	"short_name": "SK Contrast",
	"version": "1.0.1"
}
